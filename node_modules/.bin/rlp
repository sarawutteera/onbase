#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/onbase/node_modules/@ethereumjs/rlp/bin/node_modules:/workspaces/onbase/node_modules/@ethereumjs/rlp/node_modules:/workspaces/onbase/node_modules/@ethereumjs/node_modules:/workspaces/onbase/node_modules:/workspaces/node_modules:/node_modules:/workspaces/onbase/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/onbase/node_modules/@ethereumjs/rlp/bin/node_modules:/workspaces/onbase/node_modules/@ethereumjs/rlp/node_modules:/workspaces/onbase/node_modules/@ethereumjs/node_modules:/workspaces/onbase/node_modules:/workspaces/node_modules:/node_modules:/workspaces/onbase/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@ethereumjs/rlp/bin/rlp" "$@"
else
  exec node  "$basedir/../@ethereumjs/rlp/bin/rlp" "$@"
fi
